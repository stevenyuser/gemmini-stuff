   "source": [
    "import numpy as np\n",
    "\n",
    "DIM = 2\n",
    "\n",
    "a = np.empty(shape=(DIM, DIM))\n",
    "b = np.empty(shape=(DIM, DIM))\n",
    "\n",
    "for i in range(DIM):\n",
    "    for j in range(DIM):\n",
    "        a[i][j] = i + j\n",
    "        b[i][j] = i * j\n",
    "\n",
    "c = a.dot(b)\n",
    "\n",
    "print(c)"
   ]
  }
